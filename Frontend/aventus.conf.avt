{
	"module": "${{projectName}}",
	"componentPrefix": "${{componentPrefix}}",
	"hideWarnings": true,
	"build": [
		{
			"name": "Main",
			"src": [
				"./src/*"
			],
			"compile": [{
				"output": "../wwwroot/js/front.js"
			}],
			"dependances": {
				"Aventus@Sharp":{},
				"Aventus@UI": {},
				"MaterialIcon": "1.0.0"
			}
		}
	],
	"static": [
		{
			"name": "Main",
			"input": "./static/",
			"output": "../wwwroot"
		}
	]
}